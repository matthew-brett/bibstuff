  journal =      {Journal of Ecology},
  title =        {Spatial and temporal variation in disease levels of
                  a floral smut (\emph{Anthracoidea heterospora}) on
                  \emph{Carex nigra}},
  volume =       {86},
  number =       {1},
  pages =        {53-61},
  keywords =     {Disease Transmission ; Plant-Pathogen Interactions ;
                  Vector-Borne Fungal Pathogens ; Natural-Populations
                  ; Linum-Marginale ; Melampsora-Lini ; Resistance ;
                  Evolution ; Pollinators ; Consequences ; Herbivores
                  ; Parasitism},
  abstract =     {1 Data are presented from a 4-year study on the
                  incidence of infection by the nonsystemic smut
