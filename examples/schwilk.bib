@article{Vitousek+Mooney+etal:1997,
  author =       {Vitousek, P. M. and Mooney, H. A. and Lubchenco,
                  J. and Melillo, J. M.},
  year =         {1997},
  journal =      {Science},
  title =        {Human domination of earths ecosystems},
  volume =       {277},
  number =       {5325},
  pages =        {494-499},
  keywords =     {Global Change ; Biological Invasions ; Aral Sea ;
                  Nitrogen ; Fragmentation ; Deforestation ;
                  Consequences ; Biodiversity ; Management ;
                  Depletion},
  abstract =     {Human alteration of Earth is substantial and
                  growing. Between one-third and one-half of the land
                  surface has been transformed by human action; the
                  carbon dioxide concentration in the atmosphere has
                  increased by nearly 30 percent since the beginning
                  of the Industrial Revolution; more atmospheric
                  nitrogen is fixed by humanity than by all natural
                  terrestrial sources combined; more than half of all
                  accessible surface fresh water is put to use by
                  humanity; and about one-quarter of the bird species
                  on Earth have been driven to extinction. By these
                  and other standards, it is clear that we live on a
                  human-dominated planet.},
  annote =
