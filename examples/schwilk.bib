@article{Knowles:1984,
  author =       {Knowles, P.},
  year =         {1984},
  journal =      {Canadian Journal of Genetics and Cytology},
  title =        {Genetic variability among and within closely spaced
                  populations of lodgepole pine (\emph{Pinus contorta}
                  var \emph{latifolia})},
  volume =       {26},
  number =       {2},
  pages =        {177-184},
  keywords =     {rocky mountains colorado usa elevational gradient
                  allelic heterogeneity f statistics polymorphism
                  inbreeding. *Genetics and Cytogenetics/Plant [03504]
                  *Genetics and Cytogenetics/Population Genetics
                  (1972- ) [03509] *Ecology; Environmental
                  Biology/Plant [07506] *Plant Physiology,
                  Biochemistry and Biophysics/Enzymes [51518]
                  *Forestry and Forest Products [53500] Biochemical
                  Studies/Proteins, Peptides and Amino Acids [10064]
                  Enzymes/Physiological Studies [10808] Coniferopsida
                  [25102] Plants. Vascular
                  Plants. Spermatophytes. Gymnosperms.},
  abstract =     {The genetic composition of P. contorta
                  var. latifolia as determined by isozyme analysis was
                  assessed and examined at 2 hierarchical levels;
                  within closely spaced populations and within
                  adjacent subpopulations. Four populations of
                  lodgepole pine consisting of 125 trees each were
                  sampled along an elevational gradient spanning a
                  distance of .apprx. 2 km in the Front Range of the
                  Rocky Mountains in Colorado USA. Each population was
                  further divided into 5 subpopulations. Four isozymes
                  were electrophoretically resolved from needle tissue
                  removed from each tree. Statistic characterizing the
                  genetic structure were calculated including measures
                  of genetic variability, tests of allelic
                  heterogeneity and F statistics. Lodgepole pine has a
                  relatively low level of genetic variation with 44%
                  of its loci polymorphic, an average of 1.33
                  alleles/locus, heterozygous at 13.5% of their loci,
                  and a polymorphic index of 0.144. Slightly < 1% of
                  the observed genetic variability resided among the 4
                  populations with the remainder residing among the
                  individuals within populations. A slight deficiency
                  of heterozygotes was indicated by the F statistics
                  analysis. Allelic distributions at the subpopulation
                  level indicated that clustering was evident although
                  not a predominant characteristic of the genetic
                  pattern. Inbreeding and/or the pooling of
                  individuals from different breeding groups are
                  likely phenomena contributing to the genetic pattern
                  in lodgepole pine over microgeographical distances.},
  annote =       {Sch. forestry, lakehead univ., thunder bay, ont.,
                  p7b 5e1 can.},
}

