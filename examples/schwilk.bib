@article{Schwilk+Keeley+etal:1997,
  author =       {Schwilk, D. W. and Keeley, J. E. and Bond, W. J.},
  year =         {1997},
  journal =      {Plant Ecology},
  title =        {The intermediate disturbance hypothesis does not
                  explain fire and diversity pattern in fynbos},
  volume =       {132},
  number =       {1},
  pages =        {77-84},
  keywords =     {research article. cliffortia cuneata. protea
                  neriifolia. protea repens. erica
                  hispidula. terrestrial ecology. intermediate
                  disturbance hypothesis. fire. species diversity
                  pattern. species-area curve. south africa. ethiopian
                  region. *Ecology; Environmental Biology/Plant
                  [07506] Ericaceae [26035] Proteaceae [26620]
                  Rosaceae [26675] Plants. Vascular
                  Plants. Spermatophytes. Angiosperms. Dicots.},
  abstract =     {The intermediate disturbance hypothesis is a widely
                  accepted generalization regarding patterns of
                  species diversity, but may not hold true where fire
                  is the disturbance. In the Mediterranean-climate
                  shrublands of South Africa, called fynbos, fire is
                  the most importance disturbance and a controlling
                  factor in community dynamics. The intermediate
                  disturbance hypothesis states that diversity will be
                  highest at sites that have had an intermediate
                  frequency of disturbance and will be lower at sites
                  that have experienced very high or very low
                  disturbance frequencies. Measures of diversity are
                  sensitive to scale; therefore, we compared species
                  richness for three fire regimes in South African
                  mountain fynbos to test the intermediate disturbance
                  hypothesis over different spatial scales from 1 m-2
                  to 0.1 hectares. Species diversity response to fire
                  frequency was highly scale-dependent, but the
                  relationship between species diversity and
                  disturbance frequency was opposite that predicted by
                  the intermediate disturbance hypothesis. At the
                  largest spatial scales, species diversity was
                  highest at the least frequently burned sites (40
                  years between fires) and lowest at the sites of
                  moderate (15 to 26 years between fires) and high
                  fire frequency (alternating four and six year fire
                  cycle). Community heterogeneity, measured both as
                  the slope of the species-area curve for a site and
                  as the mean dissimilarity in species composition
                  among subplots within a site, correlated with
                  species diversity at the largest spatial
                  scales. Community heterogeneity was highest at the
                  least frequently burned sites and lowest at the
                  sites that experienced an intermediate fire
                  frequency.},
  annote =       {Dep. Biol. Occidental Coll., Los Angeles, CA 90041,
                  USA. Using Smart Source Parsing s English. Article.},
}

@article{Schwilk+Keeley:1998,
  author =       {Schwilk, Dylan W. and Keeley, Jon E.},
  year =         {1998},
  journal =      {Southwestern Naturalist},
  title =        {Rodent populations after a large wildfire in
                  California chaparral and coastal sage scrub},
  volume =       {43},
  number =       {4},
  pages =        {480-483},
  annote =       {Rodent populations were sampled at sites in
                  California chaparral and coastal sage scrub six
                  months after wildfire. Sites adjacent to unburned
                  brush were compared with sites in the center of
                  large burned areas. Eight species of rodents were
                  captured. Species of Peromyscus were the most
                  abundant, 40.2% of individuals captured; followed by
                  Neotoma (32.4%), Chaetodipus (20.3%), and Dipodomys
                  (7.0%). Peromyscus maniculatus accounted for 32.8%
                  of all individuals and was captured most frequently
                  at sites in the center of large burns. Neotoma
                  lepida which accounted for 31.3% of individuals
                  captured, was trapped most commonly at the periphery
                  of burns. Several species showed contrasting
                  distribution patterns in coastal sage scrub and in
                  chaparral. Coastal sage scrub sites had the highest
                  estimates of rodent populations. Species diversity
                  varied widely among sites.},
}

