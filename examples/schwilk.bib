@article{Boorse+Ewers+etal:1998,
  author =       {Boorse, G. C. and Ewers, F. W. and Davis, S. D.},
  year =         {1998},
  journal =      {American Journal of Botany},
  title =        {Response of chaparral shrubs to below-freezing
                  temperatures: Acclimation; ecotypes; seedlings
                  vs. adults},
  volume =       {85},
  number =       {9},
  pages =        {1224-1230},
  keywords =     {Ceanothus ; Chaparral ; Electrical Conductivity ;
                  Fluorescein Diacetate ; Maximum Fluorescence ; Rhus
                  (Malosma) ; Supercooling ; Variable Fluorescence ;
                  Tissue Water Relations ; Wildfire ; Fire},
  abstract =     {Leaf death due to freezing was examined for four,
                  co-occurring species of chaparral shrubs from the
                  Santa Monica Mountains of southern California, Rhus
                  laurina ( = Malosma laurina), R. ovata, Ceanothus
                  megacarpus, and C., spinosus. Measurements were made
                  on seedlings vs. adults for all species, and for
                  Rhus spp. in winter vs. summer, and at a warm vs. a
                  cold site. We used four methods to determine the
                  temperature for 50% change in activity or cell death
                  (LT50) of leaves: (1) electrical conductivity
                  (electrolyte leakage into a bathing solution), (2)
                  photosynthetic fluorescent capacity (Fv/Fm), (3)
                  percentage of palisade mesophyll cells stained by
                  fluorescein diacetate vital stain, and (4) visual
                  score of leaf color (Munsell color chart). In all
                  four species seedlings were found to be more
                  sensitive to freezing temperatures than were adults
                  by 1 degrees-3 degrees C. For adults the LT50 ranged
                  from -5 degrees C for Rhus laurina in the summer to
                  -16 degrees C for Rhus ovata in the winter. The LT50
                  of R. ovata located at a colder inland site was 4
                  degrees C lower than R, ovata at the warmer coastal
                  site just 4 km apart, suggesting ecotypic
                  differences between R. ovata at the two sites. Both
                  R. laurina and R. ovata underwent significant winter
                  hardening. At the cold site, R. ovata acclimated by
                  6 degrees C on average, while R. laurina acclimated
                  by only 3 degrees C. These results were consistent
                  with species distributions and with field
                  observations of differential shoot dieback between
                  these two congeneric species after a natural
                  freeze-thaw event in the Santa Monica Mountains.},
  annote =       {Pepperdine Univ,Div Nat Sci,Malibu,Ca 90263 ;
                  Michigan State Univ,Dept Bot & Plant Pathol,E
                  Lansing,Mi 48823 English Article Bibliography Items:
                  20 Number of times cited: 2 (c) 2002 Inst. For
                  Sci. Info},
}

@article{Borchert+Johnson+etal:2003,
  author =       {M. Borchert and M. Johnson and D.S. Schreiner and
                  S.B.V. Wall},
  title =        {Early postfire seed dispersal, seedling
                  establishment and seedling mortality of \emph{Pinus
                  coulteri} {(D.} {Don)} in central coastal
                  {California,} {USA}},
  journal =      {Plant Ecology},
  volume =       {168},
  pages =        {207--220},
  year =         {2003},
  entrydate =    {2005/05/12},
  abstract =     {Seedling recruitment in many highly serotinous
                  populations of Pinus coulteri on California's
                  central coast depends almost entirely on periodic,
                  stand-replacing fire. Compared to serotinous pines
                  of the Mediterranean Basin, little detailed
                  information is available on the postfire demography
                  of California closed-cone pines, including
                  P. coulteri. In September 1996 a wildfire burned the
                  760-ha American Canyon Research Natural Area (
                  RNA). Using aerial photography, we mapped burn
                  severity of P. coulteri-chaparral woodlands and
                  forests within the RNA. From May to September of
                  1997, we also quantified seedling establishment and
                  mortality in relation to biophysical site
                  characteristics including fire severity. Seventy-six
                  percent of P. coulteri forests and woodlands
                  experienced high-severity burns, 9%
                  moderate-severity burns, and 15% low-severity or
                  unburned. Of the 53 plots used for seedling counts,
                  70% were high-severity, 26% moderate-severity, and
                  4% low-severity. Seedling densities 13 months
                  postfire were low (0.21 m(-2)), but seedling
                  mortality also was low (8.4%). Aerial seed bank size
                  increased from north-facing to south-facing slopes
                  and from high-severity to low-severity
                  burns. Seedling recruitment was unrelated to burn
                  severity and increased with the size of the canopy
                  seed bank ( cone density). Many seedlings
                  established from rodent seed caches; 23% of the
                  seedlings established in clumps from seeds cached by
                  Dipodomys agilis, Chaetodipus californicus and
                  Peromyscus maniculatus. Pinus coulteri seeds have
                  low potential for dispersal by wind, but secondary
                  dispersal by rodents moves seeds away from source
                  trees and into neighboring chaparral. We discuss the
                  potential importance of rodent seed caching to
                  postfire demography of California and Mediterranean
                  serotinous pines.},
}

@article{Borchert+Schreiner+etal:2002,
  author =       {M. Borchert and D. Schreiner and T. Knowd and
                  T. Plumb},
  title =        {Predicting postfire survival in coulter pine
                  (\emph{Pinus coulteri}) and gray pine (\emph{Pinus
                  sabiniana}) after wildfire in central California},
  journal =      {Western Journal Of Applied Forestry},
  volume =       {17},
  pages =        {134--138},
  year =         {2002},
  entrydate =    {2005/05/05},
  abstract =     {Factors related to the survival of Coulter pine
                  (\emph{Pinus coulteri}) and gray pine (P. sabiniana)
                  3 yr after a September wildfire were evaluated for a
                  site on the central coast of California. Data for
                  146 Coulter pine and 117 gray pine were analyzed
                  with logistic regression models to estimate postfire
                  survival in relationship to tree and fire-damage
                  variables. Probability of survival,for both species
                  decreased with the increasing percentage of crown
                  scorching and increasing height of bole bark
                  char. Crown scorch was the most important
                  variable,for predicting survival of these species,
                  but it was augmented by the bole-damage
                  variable. Survival of gray pine was higher than that
